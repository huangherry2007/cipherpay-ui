# CipherPay UI Environment Configuration

# Blockchain Configuration
REACT_APP_CHAIN_TYPE=EVM
REACT_APP_RPC_URL=https://eth-mainnet.alchemyapi.io/v2/your-api-key
REACT_APP_CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890

# Relayer Configuration
REACT_APP_RELAYER_URL=https://relayer.cipherpay.com
REACT_APP_RELAYER_API_KEY=your-relayer-api-key

# Solana Configuration (if using Solana)
REACT_APP_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
REACT_APP_SOLANA_PROGRAM_ID=your-solana-program-id

# Feature Flags
REACT_APP_ENABLE_COMPLIANCE=true
REACT_APP_ENABLE_CACHING=true
REACT_APP_ENABLE_STEALTH_ADDRESSES=true

# Cache Configuration
REACT_APP_CACHE_MAX_SIZE=1000
REACT_APP_CACHE_DEFAULT_TTL=300000

# Development Configuration
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info
